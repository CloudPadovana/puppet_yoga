<% if @enable_oidc -%>
OIDCClaimPrefix          "OIDC-"
OIDCCryptoPassphrase     <%=@oidc_passphrase%>
OIDCCacheType            shm
OIDCCacheShmEntrySizeMax 65536
OIDCClaimDelimiter       ;
<% end -%><%# end of @enable_oidc %>

<% if @enable_infncloud -%>
OIDCIDTokenIatSlack      3600
<% end -%><%# end of @enable_infncloud %>

